# Copy folder with all contents

# List all open TCP/UDP ports
alias oports='netstat -atun'
# Print what using the port
alias wports='echo lsof -n -i4TCP:$1'

# Copy folder
alias cpf='cp -avr'

alias e='exit'
alias l='ls -CF --group-directories-first'
alias c='clear'
alias ls='ls --color=auto --group-directories-first'
alias grep='grep --color=auto'

alias snano='sudo nano'
